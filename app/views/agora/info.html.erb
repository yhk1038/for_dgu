    <div class="col-md-12" style="padding:0%;height:100%;margin-top: 70px;">
        <div class="col-md-2" style="background-color:#cdced0;height:1334px;padding:0%;">
            <div class="col-md-12" style="padding:0%; height:100%;position:fixed;">
                <div style="padding:0%; height:250px; border-bottom:2px solid #000;"></div>
                <p style="color:#fff; font-size:30px; font-weight:bolder;">dkdkdkdkdk</p>
            </div>
        </div>
        <div class="col-md-10" style="background-color:#E1E1E1;height:1334px; text-align:center; padding: 0%;">
            <div class="col-md-12" style="background-color:#fff; padding:10px 0px 10px 15px;z-index: 10; border-bottom: 1px solid #eeeeee;">
                <div class="col-md-12">
                    <nav id="primary-menu" class="">
        				<ul class="nav one-page-menu" data-easing="easeInOutExpo" data-speed="1250" data-offset="65" style="float:left; font-weight: bolder;width: 44.5%; font-size: 12px; text-align: center;">
        					<li style="position:relative; float: left;width: 13.5%;">
        					    <a class="black_font" href="/agora/info?category=union_groups" style="<%= "color:#1ABC9C;" if params[:category] == "union_groups" %>">
        					        <div>학과별</div></a></li>
        					<li style="position:relative; float: left;width: 13.5%;">
        					    <a class="black_font" href="/agora/info?category=create_groups" style="<%= "color:#1ABC9C;" if params[:category] == "create_groups" %>">
        					        <div>제작소</div></a></li>
        					<li style="position:relative; float: left;width: 13.5%;">
        					    <a class="black_font" href="/agora/info?category=hobby_groups" style="<%= "color:#1ABC9C;" if params[:category] == "hobby_groups" %>">
        					        <div>관심사</div></a></li>
        					<li style="position:relative; float: left;width: 13.5%;">
