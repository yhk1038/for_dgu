        					        <div>category7</div></a></li>
        				</ul>
        			</nav>
                </div>
            </div>
            <div class="col-md-12" style="height:95%; text-align:center; padding-top: 1%;">
                <% category_names = ["union_groups","create_groups","hobby_groups"] %>
                <% category_names.each do |ctname| %>
                    <% if params[:category] == "#{ctname}" %>
                        <!--임시로 자료넣음-->
                        <% if ctname == "union_groups" %>
                            <% title = "정외과 학생회 동고동락" %>
                            <% img_url = "https://politics.dongguk.edu/wp-content/uploads/2015/05/05.jpg" %>
                            <% new = 3 %>
                            <% @tmp = 1 %>
                        <% end %>
                        <% if ctname == "create_groups" %>
                            <% title = "합성 짤방" %>
                            <% img_url = "http://fimg3.pann.com/new/download.jsp?FileID=33621011" %>
                            <% new = 7 %>
                            <% @tmp = 3 %>
                        <% end %>
                        <% if ctname == "hobby_groups" %>
                            <% title = "맛집찾아" %>
                            <% img_url = "http://cfile236.uf.daum.net/image/2102834052D29C8202FF9F" %>
                            <% new = 4 %>
                            <% @tmp = 2 %>
                        <% end %>
                        <!--/임시자료-->
                        <% n = 1 %>
                        <% loop do %>
                        <% break if n > @tmp %>
                        <div class="board_card_div" style="">
                            <div style="width:100%;height:94%;padding:0%;border:0px solid #fff;background-color: #CCD3DC;">
                                <div class="board_card_div_intop" style="background-image:url(<%= img_url %>);">
                                    
                                </div>
                                <div class="board_card_div_inflow">
                                    <br>
                                    <p style="line-height: 0;"><%= title %></p>
                                    <p>새 게시물 <%= new %></p>
                                </div>
                            </div>
                        </div>
                        <% n = n+1 %>
                        <% end %>
                    <% end %>
                <% end %>
                <% i = 1 %>
                <% loop do %>
                <% break if i > (20 - @tmp.to_i) %>
                <div class="board_card_div" style="">
                    <div style="width:100%;height:94%;padding:0%;border:0px solid #fff;background-color: #CCD3DC;">
                        <div class="board_card_div_intop">
                            
                        </div>
                        <div class="board_card_div_inflow">
                            <br>
                            <p style="line-height: 0;">아 겁나졸려</p>
                            <p>왜이렇게 졸리지</p>
                        </div>
                    </div>
                </div>
                <% i = i+1 %>
                <% end %>
            </div>
